<%- include('partials/head') %>

  <body>
    <section id="container">
      <h2>Entre com seu login</h2>
      <form action="login" method="post">
        <label for="email">Usuário</label><br />
        <input type="email" id="email" name="email" placeholder="manolo@email.com" required/>
        <br />
        <label for="password">Senha</label><br />
        <input type="password" id="password" name="password" placeholder="Digite sua senha" required/><br>
        <button class="btn" type="submit">Login</button>
      </form>

      <p>Não possui uma conta?<a href="/cadastro">Clique aqui</a></p>
    </section>
  </body>
</html>
